(make <kahua-config>
  :sockbase             "unix:/usr/local/tmp/kahua"
  :working-directory    "/usr/local/var/kahua"
  :static-document-path "/var/www/kahua"
  :static-document-url  "/kahua"
  :timeout-mins         60
  :userconf-file        "/usr/local/var/kahua/user.conf"
  )

