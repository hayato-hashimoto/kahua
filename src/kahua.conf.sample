(make <kahua-config>
  :sockbase             "unix:##SOCKET_BASE_DIR##"
  :working-directory    "##localstatedir##"
  :static-document-path "/var/www/kahua"
  :static-document-url  "/kahua"
  :timeout-mins         60
  :userconf-file        "##localstatedir##/user.conf"
  )

