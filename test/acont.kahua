;-*-scheme-*-

(define (acont/start context)
        (page-frame
         '(a/cont (@@ (cont next)) "next")
         context))

(define-entry (next context)
              (page-frame
               '(a/cont "top")
               context))

(define (page-frame contents context)
        (interp-html
         `(html (body ,@contents))
         context
         (lambda (s _) s)))

(initialize-main-proc acont/start)
